{
  "version": 2,
  "builds": [{
    "src": "api/predict/index.py",
    "use": "@vercel/python",
    "config": {
      "includeFiles": ["api/predict/model.onnx", "api/predict/preprocessor_params.npz"],
      "maxLambdaSize": "200mb"
    }
  }],
  "routes": [{
    "src": "/api/predict",
    "methods": ["POST"],
    "dest": "api/predict/index.py"
  }]
}