{
  "version": 2,
  "builds": [{
    "src": "api/predict/index.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15gb",
      "includeFiles": [
        "api/predict/model.joblib",
        "api/predict/preprocessor.joblib"
      ],
      "installCommand": "pip install --extra-index-url https://download.pytorch.org/whl/cpu -r requirements.txt"
    }
  }],
  "routes": [{
    "src": "/api/predict",
    "methods": ["POST"],
    "dest": "api/predict/index.py"
  }]
}